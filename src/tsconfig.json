{
    "enableAutoDiscovery": true,
    "compileOnSave": true,

    "compilerOptions": {
        "target": "es6",
        "module": "amd",
        "outFile": "tegaki.js",
        "moduleResolution": "node",
        "newLine": "LF",
        "removeComments": true,
        "noImplicitAny": true,
        "suppressImplicitAnyIndexErrors": true,
        "noImplicitThis": true,
        "noUnusedLocals": true
    }
}
